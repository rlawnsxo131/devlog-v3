{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "@/types/*": ["@types/*"],
      "@/components/*": ["components/*"],
      "@/styles/*": ["styles/*"],
      "@/icons/*": ["image/icons/*"],
      "@/store/*": ["store/*"],
      "@/hooks/*": ["hooks/*"],
      "@/lib": ["lib"],
      "@/lib/*": ["lib/*"],
      "@/constant": ["constant"],
      "@/constant/*": ["constant/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
