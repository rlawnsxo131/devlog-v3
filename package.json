{
  "name": "devlog-v3",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "sdk": "yarn dlx @yarnpkg/sdks vscode",
    "sdk:update": "yarn dlx @yarnpkg/sdks",
    "start:dev": "next dev",
    "start:dev:https": "(local-ssl-proxy --source 3001 --target 3000) & next dev",
    "build": "next build",
    "start": "next start",
    "remove:chunk": "ts-node --transpile-only scripts/removeOldChunks.ts",
    "build:rss": "ts-node --transpile-only scripts/generateRssFeed.ts",
    "build:export": "next build && next export",
    "build:production": "NODE_ENV=production ts-node --transpile-only scripts/generateRssFeed.ts && next build && next export",
    "deploy:production": "yarn build:production && aws s3 sync out s3://devlog-v3.juntae.kim && ts-node --transpile-only scripts/removeOldChunks.ts",
    "cloudfront:invalidate": "aws cloudfront create-invalidation --distribution-id E1S24NLANHYQLI --paths '/*'",
    "lint": "next lint",
    "test": "jest --coverage"
  },
  "repository": "https://github.com/rlawnsxo131/devlog-v3",
  "author": {
    "name" : "Juntae(john) Kim",
    "email" : "public.juntae@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "nextjs",
    "blog"
  ],
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.216.0",
    "@aws-sdk/types": "^3.215.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.3",
    "@types/marked": "^4.0.7",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "feed": "^4.2.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "local-ssl-proxy": "^1.3.0",
    "marked": "^4.2.3",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@stitches/core": "^1.2.8",
    "date-fns": "^2.29.3",
    "front-matter": "^4.0.2",
    "glob": "^8.0.3",
    "next": "^12.3.1",
    "next-mdx-remote": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-katex": "^6.0.2",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-prism": "^1.3.6",
    "remark-toc": "^8.0.1",
    "sharp": "^0.31.2",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  }
}
